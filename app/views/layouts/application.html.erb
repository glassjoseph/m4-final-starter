<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
   <% flash.each do |type, message| %>
    <div class="alert alert-<%= type %>">   
    <%= sanitize(msg) %>
    </div>
  <% end %>
    </div>
  <%= yield %>
</div>

</body>
</html>
